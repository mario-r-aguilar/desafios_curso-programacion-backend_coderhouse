<div>
	<h1>Real Time Products</h1>

	{{#each products}}
		<h2>{{this.title}}</h2>
		<p>{{this.description}}</p>
		<p>{{this.code}}</p>
		<p>{{this.price}}</p>
		<p>{{this.stock}}</p>
		<p>{{this.category}}</p>
		{{#if this.thumbnails}}
			<p>{{this.thumbnails}}</p>
		{{/if}}
	{{/each}}

</div>

{{! Instanciamos socket del lado del cliente }}
<script src='/socket.io/socket.io.js'></script>
<script src='/public/js/index.js'></script>